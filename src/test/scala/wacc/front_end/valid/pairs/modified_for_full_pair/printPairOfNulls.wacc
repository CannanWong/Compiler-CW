# print a pair of null pairs

# Output:
# #addrs# = ((nil),(nil))
#

# Program:

begin
  pair(int, bool) int_bool_pair = null;
  pair(pair, pair) p = newpair(int_bool_pair, int_bool_pair) ;
  print p ;
  print " = (" ;
  pair(int, bool) q = fst p ;
  print q ;
  print "," ;
  pair(int, bool) r = snd p ;
  print r ;
  println ")"
end

